{
  "name": "digialpha",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon index.js",
    "test": "mocha --timeout 5000 --exit --recursive tests/",
    "test:unit": "cross-env NODE_ENV=test mocha tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --exit --timeout 20000 tests/integration",
    "test:watch": "cross-env NODE_ENV=test nodemon --exec mocha -R min tests/integration",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate"
  },
  "author": "kapil pandit",
  "dependencies": {
    "aws-sdk": "^2.646.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.10",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.1.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "http": "0.0.1-security",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.2.0",
    "mongoose": "^5.9.2",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.2",
    "node-input-validator": "^4.2.0",
    "node-ses": "^2.2.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "request": "^2.88.2",
    "sequelize": "^6.7.0",
    "socket.io": "^4.2.0",
    "supertest": "^6.3.3",
    "trim-request": "^1.0.6",
    "uuid-random": "^1.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "sequelize-cli": "^6.2.0"
  }
}
